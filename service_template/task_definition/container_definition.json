[
  {
    "cpu": 512,
    "essential": true,
    "image": "${image}",
    "memoryReservation": 512,
    "name": "${name}",
    "portMappings": [
      {
        "containerPort": ${containerPort}
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs-group}",
        "awslogs-region": "${awslogs-region}",
        "awslogs-stream-prefix": "${awslogs-stream-prefix}"
      }
    }
  }
]